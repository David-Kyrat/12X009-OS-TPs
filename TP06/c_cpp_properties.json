{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/**",
                "/usr/include/x86_64-linux-gnu/**",
                "/usr/lib/gcc/x86_64-linux-gnu/9/include/**"
            ],
            "defines": [],
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [
                "-std=c17",
                "-D_BSD_SOURCE"
            ],
            "cStandard": "c17",
            "mergeConfigurations": true
        }
    ],
    "version": 4
}